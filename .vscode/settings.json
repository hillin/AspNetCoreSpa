// Place your settings in this file to overwrite default and user settings.
{
    "typescript.referencesCodeLens.enabled": true,
    "debug.inlineValues": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/obj": true,
        ".npmignore": true,
        "Dockerfile": false,
        ".vs": true,
        "AspNetCoreSpa.sln": true,
        "Properties": true,
        ".awcache": true,
        "*.PublishSettings": true,
        "logs": true,
        "npm-debug.log.*": true,
        "runtimeconfig.template.json": true,
        "Client/**/*.js": true,
        "Client/**/*.js.map": true,
        "*.yml": true,
        "LICENSE": true,
        "karma.conf.js": true,
        "protractor.conf.js": true,
        "hosting.json": true,
        "NuGet.config": true,
        "global.json": true,
        "angular-cli.json": true,
        "typedoc.json": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/coverage": true,
        "compiled": false,
        "Client/**/*.js": true,
        "Client/**/*.js.map": true,
        "wwwroot": true,
        "bin": true
    },
    "typescript.tsdk": "./node_modules/typescript/lib"
}